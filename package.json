{
  "name": "saveeye-sdk-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --scheme SaveeyeSdkExample --mode Debug --extra-params \"-sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\""
  },
  "dependencies": {
    "@orbital-systems/react-native-esp-idf-provisioning": "^0.4.3",
    "@react-native-community/cli-link-assets": "^15.1.3",
    "@react-native-firebase/app": "^21.6.2",
    "@react-native-firebase/auth": "^21.14.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@saveeye/saveeye-sdk-reactnative": "^1.0.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-permissions": "^5.4.2",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^4.4.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-vision-camera": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "react-native-builder-bob": "^0.35.2",
    "reactotron-react-native": "^5.1.14"
  },
  "engines": {
    "node": ">=18"
  }
}
